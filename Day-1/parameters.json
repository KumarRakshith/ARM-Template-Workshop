{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "value": "centralindia"
        },
        "networkInterfaceName": {
            "value": "arm-sample-vm770"
        },
        "enableAcceleratedNetworking": {
            "value": true
        },
        "subnetName": {
            "value": "default"
        },
        "virtualNetworkName": {
            "value": "rak-workshop-arm-vnet"
        },
        "addressPrefixes": {
            "value": [
                "10.0.0.0/16"
            ]
        },
        "subnets": {
            "value": [
                {
                    "name": "default",
                    "properties": {
                        "addressPrefix": "10.0.0.0/24"
                    }
                }
            ]
        },
        "virtualMachineName": {
            "value": "arm-sample-vm"
        },
        "virtualMachineComputerName": {
            "value": "arm-sample-vm"
        },
        "virtualMachineRG": {
            "value": "rak-workshop-arm"
        },
        "osDiskType": {
            "value": "Premium_LRS"
        },
        "virtualMachineSize": {
            "value": "Standard_DS1_v2"
